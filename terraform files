terraform {
    required_providers {
      aws = {
            source = "hashicorp/aws"
            version = "4.50.0"
        }
    }
}
 provider "aws" {
    profile = "default"
    region = "us-east-1"
 }
resource "aws_instance" "web" {
  ami           = "ami-0b5eea76982371e91"
  instance_type = "t3.micro"

  tags = {
    Name = "HelloWorld"
  }
}
